{
  "name": "jord-jd/php-wikipedia-search",
  "description": "PHP Wikipedia Search",
  "type": "library",
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "jord-jd/php-base-search": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "JordJD\\WikipediaSearch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JordJD\\WikipediaSearch\\Tests\\": "tests/Unit/"
    }
  },
  "replace": {
    "divineomega/php-wikipedia-search": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
